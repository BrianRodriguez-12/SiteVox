{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "module": "esnext",
    "resolveJsonModule": true
  },
  "module": "es6",
  "outDir": "./dist",
  "rootDir": "./src",
  "include": ["src", "components"],
  "exclude": ["node_modules"],
  "types": [
    "cookie",
    "estree",
    "geojson",
    "json-schema",
    "leaflet",
    "node",
    "parse-json",
    "prop-types",
    "react",
    "react-dom",
    "react-transition-group",
    "trusted-types"
  ]
}
